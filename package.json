{
    "name": "electron_template",
    "version": "0.1.0",
    "description": "A generic template for an Electron application using Typescript, Vite and Forge.",
    "license": "AGPL-3.0-or-later",
    "author": {
        "name": "Mayaz Rakib",
        "email": "mayaz@rakib.com.au",
        "url": "https://mayazrakib.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mmrakib/electron_template.git"
    },
    "main": ".vite/build/main.js",
    "scripts": {
        "start": "npx electron-forge start",
        "package": "npx electron-forge package",
        "make": "npx electron-forge make",
        "publish": "npx electron-forge publish",
        "build": "npx vite build",
        "lint": "npx eslint .",
        "format": "npx prettier --write '**/*.{ts,tsx}'",
        "format-check": "npx prettier --check '**/*.{ts,tsx}'"
    },
    "devDependencies": {
        "@electron-forge/cli": "^7.8.0",
        "@electron-forge/maker-deb": "^7.8.0",
        "@electron-forge/maker-dmg": "^7.8.0",
        "@electron-forge/maker-rpm": "^7.8.0",
        "@electron-forge/maker-squirrel": "^7.8.0",
        "@electron-forge/maker-zip": "^7.8.0",
        "@electron-forge/plugin-auto-unpack-natives": "^7.8.0",
        "@electron-forge/plugin-fuses": "^7.8.0",
        "@electron-forge/plugin-vite": "^7.8.0",
        "@electron-forge/shared-types": "^7.8.0",
        "@electron/fuses": "^1.8.0",
        "@eslint/js": "^9.23.0",
        "@types/electron-squirrel-startup": "^1.0.2",
        "@types/react": "^18.3.20",
        "@types/react-dom": "^18.3.5",
        "@vanilla-extract/css": "^1.17.1",
        "@vanilla-extract/vite-plugin": "^5.0.1",
        "@vitejs/plugin-react": "^4.3.4",
        "electron": "^35.1.2",
        "eslint": "^9.23.0",
        "globals": "^16.0.0",
        "prettier": "^3.5.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.28.0",
        "vite": "^6.2.3"
    },
    "dependencies": {
        "@blueprintjs/core": "^5.17.6",
        "@blueprintjs/icons": "^5.20.0",
        "electron-is-dev": "^3.0.1",
        "electron-squirrel-startup": "^1.0.1",
        "normalize.css": "^8.0.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    }
